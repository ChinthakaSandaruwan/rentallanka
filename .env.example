# RentalLanka example environment file
# Copy this to `.env` and fill values for your environment.

BASE_URL=http://localhost/rentallanka

# Database
DB_HOST=127.0.0.1
DB_USER=your_db_user
DB_PASS=your_db_password
DB_NAME=your_db_name
DB_PERSISTENT=0

# OTP
OTP_MODE=development

# SMS (SMSlenz)
SMSLENZ_USER_ID=
SMSLENZ_API_KEY=
SMSLENZ_SENDER_ID=SMSlenzDEMO
SMSLENZ_BASE=https://smslenz.lk/api
